          //      option 1
          //      option 2
          //      option 3
          //      option 4
          //      option 5
          //    br
          //    select.form-control(multiple='')
          //      option 1
          //      option 2
          //      option 3
          //      option 4
          //      option 5

        br
.modal-footer
  .form-group
    .col-lg-10.col-lg-offset-2
      a.close-lightbox
        button.btn.btn-large.btn-default.close-lightbox(ng-click='cancel()') Cancel
      a.close-lightbox
        button.btn.btn-large.btn-primary.btn-raised.close-lightbox(ng-click='addProperty(property)') Add Property

