div.lightbox-main-content
  google-places(location='location', lat="property.latCoord", lon="property.longCoord")

    .well.bs-component.box-shadow-none
      form.form-horizontal(role='form')
        fieldset
          legend Legend
          .form-group

            // Property Type
            label.col-lg-2.control-label Type
            .col-lg-10
              .radio.radio-primary
                label
                  //input#optionsRadios1(type='radio', name='optionsRadios', value='option1', checked='')
                  input#optionsRadios1(type='radio', ng-model='property.type', name='homeOption1', ng-checked='true', value='Residential')
                  span.circle
                  span.check
                  |                                                     Residential
              .radio.radio-primary
                label
                  // input#optionsRadios2(type='radio', name='optionsRadios', value='option2')
                  input#optionsRadios2(type='radio', ng-model='property.type', name='commercialpOption2', value='Commercial')
                  span.circle
                  span.check
                  |                                                     Commercial

            // date of completion
          .form-group
            label.col-lg-2.control-label(for='') Date Picker
            .col-sm-10
              //input.form-control(type='text', datepicker-popup='{{format}}', ng-model='property.dateComplete',
              //is-open='opened', min-date='minDate', max-date="'2015-06-22'", datepicker-options='dateOptions',
              //date-disabled='disabled(date, mode)', ng-required='true', close-text='Close')
              //span.input-group-btn
              //  button.btn.btn-default(type='button', ng-click='property.openDatePicker($event)')
              //    i.glyphicon.glyphicon-calendar
              p.input-group
                input.form-control.date-picker(type='date', datepicker-popup='', ng-model='property.dateComplete', is-open='opened', min-date='minDate', max-date="'2015-06-22'", datepicker-options='dateOptions', date-disabled='disabled(date, mode)', ng-required='true', close-text='Close')
                span.input-group-btn
                  button.btn.btn-default(type='button', ng-click='open($event)')
                    i.glyphicon.glyphicon-calendar

          // Address
          .form-group
            label.col-lg-2.control-label(for='') Address
            .col-lg-10
              input#google_places_ac.form-control(type='text', name='google_places_ac', ng-model='property.address')
              //p.input-group
              //  input.form-control(type='date', datepicker-popup='', ng-model='dt', is-open='opened', min-date='minDate', max-date="'2015-06-22'", datepicker-options='dateOptions', date-disabled='disabled(date, mode)', ng-required='true', close-text='Close')
              //  span.input-group-btn
              //    button.btn.btn-default(type='button', ng-click='open($event)')
              //      i.glyphicon.glyphicon-calendar

          // Latitude
          .form-group
            label.col-lg-2.control-label(for='') Latitude
            .col-lg-10
              input.form-control(type='text', ng-model='property.latCoord')

          // Longitude
          .form-group
            label.col-lg-2.control-label(for='') Longitude
            .col-lg-10
              // Longitude
              input.form-control(type='text', ng-model='property.longCoord')

          // Improvment Size
          .form-group
            label.col-lg-2.control-label(for='') Improvement Size
            .col-lg-10
              input.form-control(type='text', ng-model='property.improvementSize')

          // City
          .form-group
            label.col-lg-2.control-label(for='') City
            .col-lg-10
              input.form-control(type='text', ng-model='property.city')

          // Acres
          .form-group
            label.col-lg-2.control-label(for='') Acres
            .col-lg-10
              input.form-control(type='text', ng-model='property.acres', placeholder=' Acres ')

          // Relevant Condition
          label.col-lg-2.control-label Relevant Condition
          .col-lg-10
            .radio.radio-primary
              label
                input#optionsRadios1(type='radio', ng-model='property.condition', name='goodOption1', ng-checked='true', value='Good')
                span.circle
                span.check
                |                                                     Good
            .radio.radio-primary
              label
                input#optionsRadios2(type='radio', ng-model='property.condition', name='poorOption2', value='Poor')
                span.circle
                span.check
                |                                                     Poor

          //.form-group
          //  label.col-lg-2.control-label(for='select') Selects
          //  .col-lg-10
